{
  "headers": [
    {
      "source": "/(.*).json",
      "headers": [
        {
          "key": "Access-Control-Allow-Origin",
          "value": "https://*.bubbleapps.io"
        },
        {
          "key": "Access-Control-Allow-Methods",
          "value": "GET"
        }
      ]
    }
  ],
  "redirects": [
    {
      "source": "/(.*).json",
      "has": [
        {
          "type": "header",
          "key": "origin",
          "value": "^(?!https://.*\\.bubbleapps\\.io).*"
        }
      ],
      "destination": "/404.html",
      "statusCode": 404
    }
  ]
}
